			"com.zbyszeq.ZbyszeqsExtraDatapackItems"
		],
		"client": [
			"com.zbyszeq.ZbyszeqsExtraDatapackItemsClient"
		]
	},
	"mixins": [
		"xtra_datapack_items.mixins.json",
		{
			"config": "xtra_datapack_items.client.mixins.json",
