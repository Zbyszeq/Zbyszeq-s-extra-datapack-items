	"authors": [
		"Zbyszeq"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/Zbyszeq/Zbyszeq-s-extra-datapack-items.git"
	},
	"license": "MIT",
